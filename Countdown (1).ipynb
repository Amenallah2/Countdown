    "\n",
    "import time\n",
    "def countdown(t):\n",
    "    a=int(t)\n",
    "    \n",
    "    while a>0:\n",
    "        timeformat= divmod(a,60)\n",
    "        print(timeformat,end=' \\r')\n",
    "        time.sleep(1)\n",
    "        a=a-1\n",
    "        \n",
    "    return ('Fire in the hole')\n",
    "\n",
    "\n",
    "\n",
    "p=str(input(''))\n",
    "print(countdown(p))        \n",
    "        \n",
